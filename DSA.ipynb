        "\n",
        "    while PQ not empty:\n",
        "        u = PQ.pop_min()\n",
        "        for v in neighbors(u):\n",
        "            if D[u] + weight(u,v) < D[v]:\n",
        "                D[v] = D[u] + weight(u,v)\n",
        "                PQ.push(v, D[v])\n",
