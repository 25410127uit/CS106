        "                d[i+1], d[high] = d[high], d[i+1]\n",
        "                return i + 1\n",
        "\n",
        "            def quick_sort_recursive(low, high):\n",
        "                if low < high:\n",
        "                    pi = partition(low, high)\n",
        "                    quick_sort_recursive(low, pi - 1)\n",
        "                    quick_sort_recursive(pi + 1, high)\n",
        "\n",
        "            quick_sort_recursive(0, n-1)\n",
        "\n",
        "        else:\n",
