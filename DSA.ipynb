        "    def run_graph_traversal(self, scr, start, mode='bfs'):\n",
        "        if start not in self.G:\n",
        "            return\n",
        "        visited = []\n",
        "        if mode == 'bfs':\n",
        "            from collections import deque\n",
        "            q = deque([start])\n",
        "            seen = set([start])\n",
        "            while q:\n",
        "                u = q.popleft()\n",
        "                visited.append(u)\n",
        "                for v in self.G.neighbors(u):\n",
        "                    if v not in seen:\n",
        "                        seen.add(v); q.append(v)\n",
        "        else:\n",
        "            stack = [start]\n",
        "            seen = set()\n",
        "            while stack:\n",
        "                u = stack.pop()\n",
        "                if u in seen:\n",
        "                    continue\n",
        "                seen.add(u)\n",
        "                visited.append(u)\n",
        "                neigh = sorted(list(self.G.neighbors(u)), reverse=True)\n",
        "                for v in neigh:\n",
        "                    if v not in seen:\n",
        "                        stack.append(v)\n",
        "\n",
        "        for i, u in enumerate(visited, 1):\n",
        "            cmap = []\n",
        "            for n in self.G.nodes():\n",
        "                if n == u:\n",
        "                    cmap.append('#f1c40f')\n",
        "                elif n in visited[:i]:\n",
        "                    cmap.append('#2ecc71')\n",
        "                else:\n",
        "                    cmap.append('#ecf0f1')\n",
        "            self.draw_graph_step(scr, color_map=cmap, title=f\"{mode.upper()} step {i}/{len(visited)}: visit {u}\")\n",
